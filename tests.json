{
	"test_httpmethods": {
		"name": "HTTP methods",
		"description": "This test simply launch GET, PUT, POST and DELETE requests",
		"type": "multicall",
		"tests": [
			{
				"send": {
					"method": "GET",
					"content": {
						"this": "is",
						"a": "test"
					}
				}
			},
			{
				"send": {
					"method": "PUT"
				}
			},
			{
				"send": {
					"method": "POST",
					"content": {
						"this": "is",
						"a": "test"
					}
				}
			},
			{
				"send": {
					"method": "DELETE"
				}
			}
		]
	},
	"test_supportedmethods": {
