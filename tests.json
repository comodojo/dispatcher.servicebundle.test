	"test_anymethod": {
		"name": "Responses to any method (wildcard)",
		"description": "Responses to any method (wildcard)",
		"type": "multicompare",
		"tests": [
			{
				"send": {
					"method": "GET"
				},
				"receive": {
					"status": 200,
					"response": [
						{
							"rule": "contain",
							"content": "GET"
						}
					]
				}
			},
			{
				"send": {
					"method": "PUT"
				},
				"receive": {
					"status": 200,
					"response": [
						{
							"rule": "contain",
							"content": "PUT"
						}
					]
				}
			},
			{
				"send": {
					"method": "POST"
				},
				"receive": {
					"status": 200,
					"response": [
						{
							"rule": "contain",
							"content": "POST"
						}
					]
				}
			},
			{
				"send": {
					"method": "DELETE"
				},
				"receive": {
					"status": 200,
					"response": [
						{
							"rule": "contain",
							"content": "DELETE"
						}
					]
				}
			}
		]
	},
